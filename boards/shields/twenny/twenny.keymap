		bindings = <&kp R>;
		layers = <0>;
	};
	combo_y {
		timeout-ms <50>;
		key-positions = <3 4>;
		bindings = <&kp Y>;
		layers = <0>;
	};
	combo_z {
		timeout-ms <50>;
		key-positions = <5 13>;
