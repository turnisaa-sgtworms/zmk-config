#include <behaviors.dtsi>
#include <dt-bindings/zmk/keys.h>



/ {
keymap {
	compatible = "zmk,keymap";
	one {
		display-name = "base";
		bindings = <
				&kp W &kp E &kp T &kp U	&kp I &kp O
				&kp A &kp S &kp D &kp F &kp H &kp K &kp L &kp P
				&kp C &kp B &kp N &kp M
				&kp SPACE &kp SPACE
			>;
		};
	two { display-name = "numico";
		bindings = <
				&kp 1 &kp 2 &kp 3 &td0 	&td1  &td2
				&kp 4 &kp 5 &kp 6 &kp 7 &kp H &kp K &kp L &kp P
				&kp 8 &kp 9 &kp N &kp M
				&kp 0 &kp SPACE
			>;
		};
	three { status = "reserved" };
	four { status = "reserved" };
	five { status = "reserved" };
	};
};



/ {
combos {
//mods
	compatible = "zmk,combos";
	combo_tab {
		timeout-ms <50>;
		key-positions = <0 2>;
		bindings = <&kp TAB>;
		layers = <0>;
	};
	combo_esc {
		timeout-ms <50>;
		key-positions = <3 5>;
		bindings = <&kp ESC>;
		layers = <0>;
	};
	combo_backspace {
		timeout-ms <50>;
		key-positions = <12 13>;
		bindings = <&kp BSPC>;
		layers = <0>;
	};
	combo_enter {
		timeout-ms <50>;
		key-positions = <9 10>;
		bindings = <&kp ENTER>;
		layers = <0>;
	};
//arrows
	combo_up {
		timeout-ms <50>;
		key-positions = <1 4>;
		bindings = <&kp UP>;
		layers = <0>;
	};
	combo_left {
		timeout-ms <50>;
