		layers = <0>;
	};
	combo_g {
		timeout-ms <50>;
		key-positions = <8 9>;
		bindings = <&kp G>;
		layers = <0>;
	};
	combo_j {
		timeout-ms <50>;
		key-positions = <10 11>;
		bindings = <&kp J>;
		layers = <0>;
	};
	combo_v {
		timeout-ms <50>;
		key-positions = <14 15>;
